{
    "name": "lord2800/dynamo",
    "description": "A generator-based lightweight framework",
    "require": {
        "php": ">=5.5.0",
        "lord2800/di.php": "master@dev"
    },
    "require-dev": {
        "sabre/http": "master@dev",
        "phpunit/phpunit": "4.0.*@dev",
        "squizlabs/php_codesniffer": "master@dev",
        "phpdocumentor/phpdocumentor": "master@dev",
        "satooshi/php-coveralls": "master@dev",
        "mikey179/vfsStream": "master@dev",
        "lord2800/convey": "master@dev"
    },
    "suggest": {
        "sabre/http": "HTTP request handling with full unit tests (required for Dynamo\\WebApp)",
        "lord2800/convey": "Lightweight flexible micro-router (required for Dynamo\\WebApp)"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Hubbard",
            "email": "lord2800@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Dynamo": "src/"
        }
    }
}
